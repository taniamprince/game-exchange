                            <xsd:enumeration value="AssuredPersistent"> 
                                <xsd:annotation> 
                                    <xsd:documentation>AssuredPersistent</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
